<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.bus" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Track Busses</name>
    <description>
        ** Track Busses **   
    </description>
    <author email="dpuljic01@gmail.com" href="https://github.com/domagojPuljic">
        Domagoj Puljic
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-geolocation" source="npm" spec="~2.1.0" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*.google.com" />
    <allow-intent href="*.googleapis.com" />
    <allow-intent href="*.gstatic.com" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
